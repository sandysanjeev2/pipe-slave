pipeline {
	agent none

		stages {
			stage('STAGE 1') {

				agent { lable 'master' }
				steps {
					echo "This is stage 1"
					sh 'sleep 5'
					}
					}
			stage('STAGE 2') {
				agent { lable 'slave1' }
				steps {
					echo "this is test2"
					sh 'sleep 5'
					}
					}
			stage('STAGE 3') {
				agent {lable 'slave1' }
				steps {
					echo "This is from test3"
					sh 'sleep 5'
					}
				}
		}
	}
