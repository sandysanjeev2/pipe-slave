pipeline {
	agent none

		stages {
			stage('test') {
				agent { label 'master' }
				steps {
					echo "This is stage 1"
					sh 'sleep 5'
					}
					}
			stage('test2') {
				agent { lable 'slave1' }
				steps {
					echo "this is test2"
					sh 'sleep 5'
					}
					}
			stage('test3') {
				agent {label 'slave1' }
				steps {
					echo "This is from test3"
					sh 'sleep 5'
					}
				}
		}
	}
